javascript:var num=prompt("How many times do you want this page to show up in your history?\nMade by: THH");done=false;x=window.location.href;for (var i=1; i<=num; i++){history.pushState(0, 0, i==num?x:i.toString());if(i==num){done=true}}if(done===true){alert("Flooded Successfully!\n "+window.location.href+" \nwill appaer in your history "+num+(num==1?" time.":" times.?\nCtrl+H / Command+H to open history"))}
